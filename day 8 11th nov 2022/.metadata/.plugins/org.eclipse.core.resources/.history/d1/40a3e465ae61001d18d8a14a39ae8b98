package com.Assignment7.main;

import com.Assignmet7.Entity.Account;
import com.Assignmet7.Entity.PrevilegedAccount;
import com.Assignmet7.Entity.SavingsAccount;

public class AppMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Account a1 = new Account(10, "dhiraj", "ProvilegedAccount", 0.00);
		a1.deposit(0);
		a1.withdraw(0);
		a1.checkBalance();
		
		
		SavingsAccount s1 = new SavingsAccount(10, "Dhiraj", "Savings", 1100.00);
		
		s1.deposit(1000);
		s1.checkBalance();
		s1.withdraw(0);
		
		PrevilegedAccount p1 = new PrevilegedAccount(0, null, null, null);
		
		p1.deposit(0);
		p1.withdraw(0);
		p1.checkBalance();
	}

}
