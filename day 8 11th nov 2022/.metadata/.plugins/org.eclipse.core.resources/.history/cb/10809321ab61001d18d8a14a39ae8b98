package com.Assignment7.main;

import com.Assignmet7.Entity.Account;
import com.Assignmet7.Entity.SavingsAccount;

public class AppMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Account a1 = new Account(10, "dhiraj", "ProvilegedAccount", 10000.00);
		a1.deposit(10000);
		a1.withdraw(500);
		
		SavingsAccount s1 = new SavingsAccount(0, null, null, null);
		
		s1.deposit(0);
		s1.checkBalance();
		s1.withdraw(0);
	}

}
