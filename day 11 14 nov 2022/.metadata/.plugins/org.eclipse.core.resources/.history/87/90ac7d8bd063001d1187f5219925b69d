package com.objectdemo.pojo;

public class Student {
	
	int rollno ;
	String name ;
	
	public Student(int rollno, String name) {
		this.rollno = rollno;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
}
