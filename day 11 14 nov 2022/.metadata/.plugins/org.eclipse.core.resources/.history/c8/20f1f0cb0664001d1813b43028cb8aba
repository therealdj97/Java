package com.Assignment9.main;

public class AppMain4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int r;
		int sum =0;
		int temp;
		String s1 ="nitin";
		temp = s1;
		
		while(s1>0) {
			r=s1%10;
			sum=(sum*10)+r;
			s1=s1/10;
		}
		if(temp == sum) {
			System.out.println("number is palindrome");
		}
		else
			System.out.println("number isn't palindrome");
	}
}
