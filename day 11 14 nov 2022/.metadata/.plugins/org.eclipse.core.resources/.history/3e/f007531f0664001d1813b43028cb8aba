package com.Assignment9.main;

public class AppMain4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int r;
		int sum =0;
		int temp;
		int n =545;
		temp = n;
		
		while(n>0) {
			r=n%10;
			sum=(sum*10)+r;
			n=n/10;
		}
		if(temp == sum) {
			System.out.println("number is palindrome");
		}
		else
			System.out.println("number isn't palindrome");
	}
	

}
