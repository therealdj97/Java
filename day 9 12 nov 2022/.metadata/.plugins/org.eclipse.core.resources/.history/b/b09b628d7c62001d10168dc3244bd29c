package com.Assignments8.main;
import com.Assignment8.implemet.Verify;

public class AppMainQ5 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Verify ref =  AppMainQ5 :: isPrime;
		System.out.println(ref.check(20));
	}
	
	public static boolean isPrime(int a) {
		boolean flag = false;
		for(int i=0;i<a/2;i++) {
			if(a%i==0) {
				flag = false;
				break;
			}
			else
				flag = true;		
		}
		return flag;
	}
	
	public boolean odd(int a) {
		if(a%2==0) {
			return true;
		}
		else
			return false;
	}

}
